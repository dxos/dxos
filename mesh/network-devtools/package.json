    "ts-loader": "~8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "16.12.0",
    "react": "16.13.0",
    "typescript": "^4.0.2",
    "@dxos/react-ux": "2.7.44",
    "eslint": "^7.7.0"
  },
  "dependencies": {
