{
  "name": "@dxos/protocol-plugin-presence",
  "version": "1.0.8",
  "description": "Protocol plugin presence.",
  "homepage": "https://github.com/dxos/protocol/tree/master/packages/protocol-plugin-presence#readme",
  "bugs": {
    "url": "https://github.com/dxos/protocol/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/protocol.git"
  },
  "license": "GPL-3.0",
  "files": [
    "dist",
    "index.js"
  ],
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:protobuf && pnpm run build:babel",
    "build:babel": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\" --source-maps",
    "build:protobuf": "pbjs -t json src/schema.proto -o src/schema.json && cpy src/schema.json dist",
    "clean": "del-cli dist",
    "coverage": "pnpm test -- --coverage",
    "coveralls": "pnpm run coverage && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "pnpm run build && pnpm run test",
    "test": "jest --verbose --passWithNoTests src"
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@dxos/broadcast": "~1.0.0-beta.11",
    "@dxos/codec-protobuf": "*",
    "@dxos/protocol": "~1.0.8",
    "buffer-json-encoding": "^1.0.2",
    "debug": "^4.1.1",
    "ngraph.graph": "^19.0.1",
    "p-limit": "^3.0.2",
    "queue-microtask": "^1.1.2",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "~7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.4.5",
    "@dxos/protocol-network-generator": "1.0.8",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-inline-import": "^3.0.0",
    "coveralls": "^3.0.7",
    "cpy-cli": "^2.0.0",
    "del-cli": "^3.0.0",
    "jest": "^26.2.2",
    "ngraph.path": "^1.2.0",
    "wait-for-expect": "^3.0.2",
    "estraverse": "^5.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
