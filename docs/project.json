{
  "projectType": "application",
  "targets": {
    "apidoc": {
      "dependsOn": [
        "^build",
        "typedoc"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "apidoc"
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "bundle": {
      "dependsOn": [
        "^build",
        "^bundle",
        "build",
        "prebuild"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "bundle"
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "docs/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "prerelease": {
      "executor": "nx:run-script",
      "options": {
        "script": "prerelease"
      },
      "outputs": [
        "docs/docs",
        "docs/docs/.vuepress/dist"
      ]
    },
    "serve": {
      "dependsOn": [
        "^build"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "serve"
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "configDir": "docs/src/stories/react/.storybook"
      }
    },
    "storybook-build": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:build",
      "options": {
        "configDir": "docs/src/stories/react/.storybook",
        "outputDir": "docs/src/stories/react/out"
      },
      "outputs": [
        "{options.outputDir}"
      ]
    },
    "toolbox": {
      "executor": "@dxos/toolbox:exec"
    },
    "typedoc": {
      "dependsOn": [
        "^build"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "typedoc"
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "root": "docs"
}
