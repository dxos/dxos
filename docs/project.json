{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "docs",
  "projectType": "application",
  "targets": {
    "prebuild": {
      "dependsOn": [
        "^typedoc"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild"
      },
      "outputs": [
        "{projectRoot}/public/typedoc"
      ]
    },
    "build": {},
    "bundle": {
      "dependsOn": [
        "build",
        "^build"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "bundle"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "lint": {
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "{projectRoot}/src/**/*.{ts,tsx,js,jsx}"
        ],
        "quiet": true
      }
    },
    "pack": {},
    "serve": {
      "dependsOn": [
        "^build",
        "prebuild"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "start"
      }
    }
  }
}
