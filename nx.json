{
  "extends": "nx/presets/npm.json",
  "npmScope": "dxos",
  "affected": {
    "defaultBase": "main"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "bundle",
          "check",
          "e2e",
          "lint",
          "prebuild",
          "prerelease",
          "proto",
          "test"
        ],
        "parallel": 6,
        "accessToken": "ZGVlNTQ0ODctNGI4ZS00MTc0LTljZDQtMzFlMzJkOTViMmQyfHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build",
        "^bundle",
        "proto",
        "prebuild"
      ]
    },
    "bundle": {
      "inputs": [
        "default",
        "^default",
        {
          "env": "CONFIG_DYNAMIC"
        }
      ],
      "dependsOn": [
        "^bundle",
        "build",
        "proto",
        "prebuild"
      ]
    },
    "check": {
      "dependsOn": [
        "lint",
        "test",
        "proto",
        "build"
      ]
    },
    "e2e": {
      "dependsOn": [
        "proto",
        "build",
        "bundle"
      ]
    },
    "prebuild": {
      "dependsOn": [
        "^build"
      ]
    },
    "proto": {
      "dependsOn": [
        "^build"
      ]
    },
    "serve": {
      "dependsOn": [
        "^build",
        "^bundle",
        "proto"
      ]
    },
    "test": {
      "inputs": [
        "default",
        "^default",
        {
          "env": "CI"
        },
        {
          "env": "LOG_FILTER"
        },
        {
          "env": "LOG_PROCESSOR"
        }
      ],
      "dependsOn": [
        "^build",
        "proto"
      ]
    }
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    }
  }
}
