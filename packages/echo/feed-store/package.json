    "@types/node": "^16.11.27",
    "@types/pify": "^3.0.2",
    "@types/pump": "^1.1.0",
    "@types/through2": "^2.0.36",
    "end-of-stream-promise": "^1.0.0",
    "eslint": "^7.12.1",
    "hypertrie": "5.1.1",
    "tempy": "~1.0.1",
    "typescript": "^4.7.2",
    "wait-for-expect": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
