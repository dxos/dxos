    "debug": "^4.3.3",
    "end-of-stream": "^1.4.1",
    "from2": "^2.3.0",
    "hypercore": "^9.10.0",
    "pify": "~5.0.0",
    "pump": "^3.0.0",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "@dxos/benchmark-suite": "^1.0.0-beta.1",
    "@dxos/browser-runner": "^1.0.0-beta.13",
    "@dxos/crypto": "workspace:*",
    "@dxos/eslint-plugin": "~1.0.40",
