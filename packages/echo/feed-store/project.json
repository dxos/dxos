{
  "sourceRoot": "packages/echo/feed-store/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/echo/feed-store/dist",
        "tsConfig": "packages/echo/feed-store/tsconfig.json",
        "main": "packages/echo/feed-store/src/index.ts",
        "updateBuildableProjectDepsInPackageJson": false,
        "assets": ["packages/echo/feed-store/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/echo/feed-store/**/*.{ts,js}"
        ]
      }
    }
  }
}
