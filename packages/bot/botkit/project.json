{
  "sourceRoot": "packages/bot/botkit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "packages/bot/botkit/dist",
        "packages/bot/botkit/src/proto/gen"
      ],
      "options": {
        "script": "build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/bot/botkit/**/*.{ts,js}"
        ]
      }
    }
  }
}
