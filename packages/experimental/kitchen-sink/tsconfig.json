    {
      "path": "../../common/util"
    },
    {
      "path": "../../core/echo/echo-db"
    },
    {
      "path": "../../core/echo/object-model"
    },
    {
      "path": "../../core/protocols"
    },
    {
      "path": "../../deprecated/react-components"
    },
    {
      "path": "../../deprecated/react-toolkit"
    },
    {
      "path": "../../sdk/client"
    },
    {
      "path": "../../sdk/config"
    },
    {
      "path": "../../sdk/react-client"
    },
    {
      "path": "../client-testing"
    },
    {
      "path": "../gem-core"
    },
    {
      "path": "../gem-spore"
    },
    {
