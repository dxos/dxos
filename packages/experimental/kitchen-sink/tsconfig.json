{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "dist",
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2018",
    "types": [
      "mocha",
      "node"
    ]
  },
  "include": [
    "playwright",
    "src",
    "stories"
  ],
