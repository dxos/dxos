{
  "sourceRoot": "packages/experimental/ui-consumer/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "packages/experimental/ui-consumer/out/ui-consumer",
        "configFile": "packages/experimental/ui-consumer/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/experimental/ui-consumer/src/**/*.{js,jsx,ts,tsx}"
        ]
      }
    }
  }
}
