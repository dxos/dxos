{
  "sourceRoot": "packages/experimental/react-echo-graph/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/experimental/react-echo-graph/src/index.ts",
        "outputPath": "packages/experimental/react-echo-graph/dist",
        "tsConfig": "packages/experimental/react-echo-graph/tsconfig.json",
        "transformers": [
          "@dxos/log-hook/transformer"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/experimental/react-echo-graph/**/*.{ts,tsx,js,jsx}"
        ],
        "format": "unix"
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "toolbox": {
      "executor": "@dxos/toolbox:exec"
    }
  }
}
