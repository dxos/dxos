    "react-chessboard": "^4.7.1",
    "react-resize-detector": "^11.0.1",
    "react-router-dom": "^6.4.0"
  },
  "devDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/react-ui-theme": "workspace:*",
    "@dxos/storybook-utils": "workspace:*",
    "@types/chess.js": "^0.13.4",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "vite": "5.4.7"
