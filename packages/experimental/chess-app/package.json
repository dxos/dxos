{
  "name": "@dxos/chess-app",
  "version": "0.6.12",
  "private": true,
  "description": "Chess app.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "exports": {
    ".": {
      "browser": "./dist/lib/browser/index.mjs",
      "node": {
