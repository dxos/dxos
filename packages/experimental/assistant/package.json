{
  "name": "@dxos/assistant",
  "version": "0.8.1",
  "description": "Natural language interface to DXOS",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "sideEffects": true,
  "type": "module",
  "imports": {
    "#parjs": {
      "browser": "./vendor/parjs-browser.mjs",
      "default": "./vendor/parjs-default.mjs"
    },
    "#parjs/combinators": {
      "browser": "./vendor/parjs-combinators-browser.mjs",
      "default": "./vendor/parjs-combinators-default.mjs"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs",
      "node": "./dist/lib/node-esm/index.mjs"
    },
    "./testing": {
      "types": "./dist/types/src/testing/index.d.ts",
      "browser": "./dist/lib/browser/testing/index.mjs",
      "node": "./dist/lib/node-esm/testing/index.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist",
    "vendor",
    "src"
  ],
  "scripts": {
    "run:chat": "tsx src/bin/chat.ts"
  },
  "dependencies": {
    "@dxos/artifact": "workspace:*",
    "@dxos/async": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/echo-schema": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/live-object": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/util": "workspace:*",
    "parjs": "^1.3.9",
    "parsimmon": "^1.18.1",
    "ulidx": "^2.3.0"
  },
  "devDependencies": {
    "@types/parsimmon": "^1.10.9",
    "effect": "3.13.3",
    "inquirer": "^12.0.0"
  },
  "peerDependencies": {
    "effect": "^3.13.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "beast": {}
}
