{
  "sourceRoot": "packages/gravity/proto-guard/src",
  "projectType": "library",
  "targets": {
    "compile": {
      "executor": "@dxos/esbuild:build",
      "options": {
        "entryPoints": [
          "packages/gravity/proto-guard/src/index.ts",
          "packages/gravity/proto-guard/src/main.ts"
        ],
        "outputPath": "packages/gravity/proto-guard/dist/lib"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/gravity/proto-guard/**/*.{ts,tsx,js,jsx}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@dxos/test:run",
      "options": {
        "coveragePath": "coverage/packages/gravity/proto-guard",
        "outputPath": "tmp/mocha/packages/gravity/proto-guard",
        "resultsPath": "test-results/packages/gravity/proto-guard",
        "testPatterns": [
          "packages/gravity/proto-guard/src/**/*.test.{ts,js}"
        ],
        "watchPatterns": [
          "packages/gravity/proto-guard/src/**/*"
        ]
      },
      "outputs": [
        "{options.coveragePath}",
        "{options.outputPath}",
        "{options.resultsPath}"
      ]
    }
  }
}
