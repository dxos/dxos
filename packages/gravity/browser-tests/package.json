{
  "name": "@dxos/browser-tests",
  "version": "2.10.10",
  "private": true,
  "description": "Browser tests for SDK.",
  "bugs": {
    "url": "'https://github.com/dxos/protocols/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/protocols.git"
  },
  "license": "AGPL-3.0",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "",
    "build:test": "browser-mocha --setup test/setup.js 'test/*.test.ts'",
    "lint": "toolchain lint",
    "test": "pnpm run test:chromium && pnpm run test:firefox",
    "test:chromium": "browser-mocha --setup test/setup.js 'test/**/*.test.ts' --browsers=chromium",
    "test:firefox": "browser-mocha --setup test/setup.js 'test/**/*.test.ts' --browsers=firefox"
  },
  "devDependencies": {
    "@dxos/toolchain-node-library": "workspace:*",
    "@dxos/browser-mocha": "workspace:*",
    "mocha": "~8.4.0",
    "@types/mocha": "~8.2.2",
    "expect": "~27.0.2"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "source-map-support": "^0.5.12",
    "@dxos/echo-db": "workspace:*",
    "@dxos/object-model": "workspace:*",
    "@dxos/credentials": "workspace:*"
  }
}
