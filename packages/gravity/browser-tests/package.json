{
  "name": "@dxos/browser-tests",
  "version": "2.33.9",
  "private": true,
  "description": "Browser tests for SDK.",
  "bugs": {
    "url": "https://github.com/dxos/protocols/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/protocols.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "toolchain build",
    "build:test": "toolchain build:test",
    "lint": "toolchain lint",
    "test": "toolchain test",
    "test:browser": "test $(uname -r) = '21.1.0' || browser-mocha --setup browser-test/setup.js 'browser-test/**/*.test.ts' --checkLeaks=false --browser=chromium --browser=firefox --browser=webkit"
  },
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/credentials": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/object-model": "workspace:*",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@dxos/browser-mocha": "workspace:*",
    "@dxos/eslint-plugin": "~1.0.34",
    "@dxos/protocols-toolchain": "workspace:*",
    "@dxos/protocols": "workspace:*",
    "@dxos/random-access-multi-storage": "workspace:*",
    "@dxos/signal": "workspace:*",
    "@types/mocha": "^8.2.2",
    "eslint": "^7.12.1",
    "expect": "~27.0.2",
    "mocha": "^8.4.0",
    "typescript": "^4.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "toolchain": {
    "additionalTestSteps": [
      "test:browser"
    ]
  }
}
