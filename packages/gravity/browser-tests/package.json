{
  "name": "@dxos/browser-tests",
  "version": "2.10.10",
  "private": true,
  "description": "Browser tests for SDK.",
  "bugs": {
    "url": "'https://github.com/dxos/protocols/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/protocols.git"
  },
  "license": "AGPL-3.0",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "toolchain build",
    "build:test": "toolchain build:test",
    "lint": "toolchain lint",
    "test": "toolchain test",
    "test:browser": "browser-mocha --setup browser-test/setup.js 'browser-test/**/*.test.ts' --checkLeaks=false --browser=chromium"
  },
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/credentials": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/object-model": "workspace:*",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@dxos/browser-mocha": "workspace:*",
    "@dxos/signal": "workspace:*",
    "@dxos/toolchain-node-library": "workspace:*",
    "@types/mocha": "~8.2.2",
    "expect": "~27.0.2",
    "mocha": "~8.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "toolchain": {
    "additionalTestSteps": [
      "test:browser"
    ]
  }
}
