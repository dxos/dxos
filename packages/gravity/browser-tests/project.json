{
  "sourceRoot": "packages/gravity/browser-tests/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/gravity/browser-tests/dist",
        "tsConfig": "packages/gravity/browser-tests/tsconfig.json",
        "main": "packages/gravity/browser-tests/src/index.ts",
        "updateBuildableProjectDepsInPackageJson": false,
        "assets": [
          "packages/gravity/browser-tests/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/gravity/browser-tests/**/*.{ts,js}"
        ]
      }
    },
    "e2e": {
      "executor": "@dxos/mocha:browser",
      "options": {
        "testPatterns": [
          "packages/gravity/browser-tests/browser-test/**/*.test.ts"
        ],
        "setup": "packages/gravity/browser-tests/browser-test/setup.js"
      }
    }
  },
  "implicitDependencies": [
    "mocha"
  ]
}
