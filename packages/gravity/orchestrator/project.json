{
  "sourceRoot": "packages/gravity/orchestrator/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": [
          "packages/gravity/orchestrator/*.md"
        ],
        "main": "packages/gravity/orchestrator/src/index.ts",
        "outputPath": "packages/gravity/orchestrator/dist",
        "tsConfig": "packages/gravity/orchestrator/tsconfig.json",
        "updateBuildableProjectDepsInPackageJson": false,
        "transformers": [
          "@dxos/log-hook/transformer"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/gravity/orchestrator/**/*.{ts,js}"
        ],
        "format": "unix"
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "toolbox": {
      "executor": "@dxos/toolbox:exec"
    }
  },
  "implicitDependencies": [
    "mocha"
  ]
}
