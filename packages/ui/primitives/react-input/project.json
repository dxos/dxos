{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "react-input",
  "tags": [
    "scope:primitives"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {},
    "compile": {
      "dependsOn": [
        "compile-lib"
      ],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "true"
        ]
      }
    },
    "compile-lib": {
      "dependsOn": [
        "^compile"
      ],
      "executor": "@dxos/esbuild:build",
      "options": {
        "entryPoints": [
          "{projectRoot}/src/index.ts"
        ],
        "outputPath": "{projectRoot}/dist/lib",
        "platforms": [
          "browser",
          "node"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {}
  },
  "implicitDependencies": [
    "esbuild",
    "node-std"
  ]
}
