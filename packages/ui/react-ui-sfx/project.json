{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "react-ui-sfx",
  "tags": ["scope:ui"],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "options": {
          "command": "pnpm run glsl"
        }
      }
    },
    "build": {},
    "compile": {
      "options": {
        "entryPoints": ["{projectRoot}/src/index.ts"],
        "injectGlobals": true
      }
    },
    "lint": {}
  },
  "implicitDependencies": ["esbuild", "node-std"]
}
