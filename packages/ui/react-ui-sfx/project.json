{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "react-ui-sfx",
  "tags": [
    "scope:ui"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {},
    "compile": {
      "options": {
        "entryPoints": [
          "{projectRoot}/src/index.ts"
        ],
        "injectGlobals": true
      }
    },
    "lint": {},
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run glsl",
        "cwd": "{projectRoot}"
      }
    }
  },
  "implicitDependencies": [
    "esbuild",
    "node-std"
  ]
}
