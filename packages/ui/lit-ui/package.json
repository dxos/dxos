{
  "name": "@dxos/lit-ui",
  "version": "0.8.3",
  "description": "Web Components for DXOS using Lit",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/src/index.js",
      "node": "./dist/src/index.js"
    },
    "./react": {
      "types": "./dist/types/src/react.d.ts",
      "browser": "./dist/src/react.js",
      "node": "./dist/src/react.js"
    },
    "./dx-avatar.pcss": "./src/dx-avatar/dx-avatar.pcss",
    "./dx-tag-picker.pcss": "./src/dx-tag-picker/dx-tag-picker.pcss",
    "./dx-ref-tag.pcss": "./src/dx-ref-tag/dx-ref-tag.pcss"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "./dist/types/src/react.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@dxos/react-hooks": "workspace:*",
    "@dxos/util": "workspace:*",
    "@lit/react": "^1.0.5",
    "i18next": "^21.10.0",
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "@dxos/test-utils": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
