    "vite": "5.4.7"
  },
  "peerDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/react-ui-theme": "workspace:*",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
