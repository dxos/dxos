{
  "sourceRoot": "packages/common/node-std/src",
  "projectType": "library",
  "targets": {
    "compile": {
      "executor": "@dxos/esbuild:build",
      "options": {
        "bundlePackages": [
          "assert",
          "available-typed-arrays",
          "call-bind",
          "define-properties",
          "define-data-property",
          "es-abstract",
          "es6-object-assign",
          "events",
          "for-each",
          "function-bind",
          "get-intrinsic",
          "gopd",
          "has",
          "has-property-descriptors",
          "has-proto",
          "has-symbols",
          "has-tostringtag",
          "inherits",
          "is-arguments",
          "is-callable",
          "is-generator-function",
          "is-nan",
          "is-typed-array",
          "object-is",
          "object-keys",
          "path-browserify",
          "string_decoder",
          "util",
          "util-deprecate",
          "which-typed-array"
        ],
        "entryPoints": [
          "packages/common/node-std/src/assert.js",
          "packages/common/node-std/src/crypto.js",
          "packages/common/node-std/src/events.js",
          "packages/common/node-std/src/fs.js",
          "packages/common/node-std/src/globals.js",
          "packages/common/node-std/src/path.js",
          "packages/common/node-std/src/stream.js",
          "packages/common/node-std/src/util.js"
        ],
        "outputPath": "packages/common/node-std/dist/lib",
        "platforms": [
          "browser"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/common/node-std/**/*.{ts,js}?(x)"
        ],
        "quiet": true
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
