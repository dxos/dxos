{
  "name": "@dxos/node-std",
  "version": "0.1.52",
  "description": "Simple wrapper for browser replacements of node stdlib modules within the DXOS codebase.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "exports": {
    "./assert.js": {
      "browser": "./dist/lib/browser/assert.js.mjs",
      "import": "./dist/lib/browser/assert.js.mjs"
    },
    "./events.js": {
      "browser": "./dist/lib/browser/events.js.mjs",
      "import": "./dist/lib/browser/events.js.mjs"
    },
    "./fs.js": {
      "browser": "./dist/lib/browser/fs.js.mjs",
      "import": "./dist/lib/browser/fs.js.mjs"
    },
    "./globals.js": {
      "browser": "./dist/lib/browser/globals.js.mjs",
      "import": "./dist/lib/browser/globals.js.mjs"
    },
    "./path.js": {
      "browser": "./dist/lib/browser/path.js.mjs",
      "import": "./dist/lib/browser/path.js.mjs"
    },
    "./stream.js": {
      "browser": "./dist/lib/browser/stream.js.mjs",
      "import": "./dist/lib/browser/stream.js.mjs"
    },
    "./util.js": {
      "browser": "./dist/lib/browser/util.js.mjs",
      "import": "./dist/lib/browser/util.js.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "readable-stream": "^3.6.0",
    "util": "^0.12.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "assert.js": [
        "dist/types/src/assert.js.d.ts"
      ],
      "events.js": [
        "dist/types/src/events.js.d.ts"
      ],
      "fs.js": [
        "dist/types/src/fs.js.d.ts"
      ],
      "globals.js": [
        "dist/types/src/globals.js.d.ts"
      ],
      "path.js": [
        "dist/types/src/path.js.d.ts"
      ],
      "stream.js": [
        "dist/types/src/stream.js.d.ts"
      ],
      "util.js": [
        "dist/types/src/util.js.d.ts"
      ]
    }
  }
}