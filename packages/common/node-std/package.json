{
  "name": "@dxos/node-std",
  "version": "0.8.3",
  "description": "Simple wrapper for browser replacements of node stdlib modules within the DXOS codebase.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    "./_/config": "./dist/lib/browser/_/config.js",
    "./assert": "./dist/lib/browser/assert.js",
    "./buffer": "./dist/lib/browser/buffer.js",
    "./crypto": "./dist/lib/browser/crypto.js",
    "./events": "./dist/lib/browser/events.js",
    "./fs": "./dist/lib/browser/fs.js",
    "./fs/promises": "./dist/lib/browser/fs/promises.js",
    "./inject-globals": "./dist/lib/browser/inject-globals.js",
    "./path": "./dist/lib/browser/path.js",
    "./stream": "./dist/lib/browser/stream.js",
    "./util": "./dist/lib/browser/util.js"
  },
  "typesVersions": {
    "*": {
      "_/config": [
        "types/_/config.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "assert": "^2.0.0",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "readable-stream": "^3.6.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}