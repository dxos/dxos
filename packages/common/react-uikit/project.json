{
  "sourceRoot": "packages/common/react-uikit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/common/react-uikit/dist",
        "tsConfig": "packages/common/react-uikit/tsconfig.json",
        "main": "packages/common/react-uikit/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/common/react-uikit/src/**/*.{js,jsx,ts,tsx}"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "config": {
          "configFolder": "packages/common/react-uikit/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
