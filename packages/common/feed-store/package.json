{
  "name": "@dxos/feed-store",
  "version": "0.8.3",
  "description": "A consistent store for hypercore feeds.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs",
      "node": "./dist/lib/node-esm/index.mjs"
    },
    "./testing": {
      "types": "./dist/types/src/testing/index.d.ts",
      "browser": "./dist/lib/browser/testing/index.mjs",
      "node": "./dist/lib/node-esm/testing/index.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "testing": [
        "dist/types/src/testing/index.d.ts"
      ]
    }
  },
  "files": [
    "testing.d.ts",
    "testing.js",
    "dist",
    "src"
  ],
  "dependencies": {
    "@dxos/async": "workspace:*",
    "@dxos/codec-protobuf": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/hypercore": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/keyring": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/random-access-storage": "workspace:*",
    "@dxos/util": "workspace:*",
    "@dxos/vendor-hypercore": "workspace:*",
    "lodash.defaultsdeep": "catalog:",
    "race-as-promised": "catalog:",
    "streamx": "catalog:"
  },
  "devDependencies": {
    "@dxos/keys": "workspace:*",
    "@dxos/random": "workspace:*",
    "@types/lodash.defaultsdeep": "catalog:"
  },
  "optionalDependencies": {
    "@dxos/random": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "beast": {
    "classDiagrams": [
      {
        "root": "src/feed-store.ts"
      },
      {
        "root": "src/feed-iterator.ts"
      }
    ]
  },
  "sourceExports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./testing": {
      "default": "./src/testing/index.ts"
    }
  }
}
