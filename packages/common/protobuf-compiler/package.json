{
  "name": "@dxos/protobuf-compiler",
  "version": "2.33.8",
  "license": "MIT",
  "author": "DXOS.org",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "build-protobuf": "./bin/main.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm run gen && tsc",
    "build:test": "pnpm run build && pnpm run lint && pnpm run test",
    "gen": "ts-node src/main.ts test/proto/schema.proto test/proto/another.proto -s test/proto/substitutions.ts -o test/proto/gen",
    "lint": "eslint 'src/**/*.ts'",
    "test": "mocha -r @swc-node/register '**/*.test.ts'"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "chalk": "^4.1.0",
    "protobufjs": "^6.9.0",
    "read-pkg": "^5.2.0",
    "ts-morph": "~10.0.2"
  },
  "devDependencies": {
    "@dxos/codec-protobuf": "workspace:*",
    "@dxos/eslint-plugin": "~1.0.40",
    "@dxos/protobuf-test": "workspace:*",
    "@types/argparse": "^2.0.0",
    "@types/node": "^16.11.27",
    "eslint": "^7.12.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.2",
    "mocha": "~8.4.0",
    "@types/mocha": "~8.2.2",
    "expect": "~27.0.2",
    "@swc-node/register": "~1.4.2"
  },
  "peerDependencies": {
    "typescript": "^4.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
