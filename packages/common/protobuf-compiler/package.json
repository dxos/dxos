{
  "name": "@dxos/protobuf-compiler",
  "version": "2.12.2",
  "license": "MIT",
  "bin": {
    "build-protobuf": "./bin/main.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm run gen && tsc",
    "build:test": "pnpm run build && pnpm run test",
    "gen": "ts-node src/main.ts test/schema.proto test/another.proto -s test/substitutions.ts -o test/gen",
    "test": "pnpm run lint && jest 2>&1",
    "lint": "eslint 'src/**/*.ts'"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "test/gen",
      "dist"
    ],
    "preset": "ts-jest/presets/default",
    "testEnvironment": "node"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "chalk": "^4.1.0",
    "protobufjs": "^6.9.0",
    "read-pkg": "^5.2.0",
    "ts-morph": "~10.0.2"
  },
  "devDependencies": {
    "@dxos/codec-protobuf": "workspace:*",
    "@dxos/eslint-plugin": "~1.0.11",
    "@types/argparse": "^2.0.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.9",
    "eslint": "^7.7.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
