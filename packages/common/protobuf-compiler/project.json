{
  "sourceRoot": "packages/common/protobuf-compiler/src",
  "projectType": "library",
  "targets": {
    "compile": {
      "executor": "@nx/js:tsc",
      "options": {
        "main": "packages/common/protobuf-compiler/src/index.ts",
        "outputPath": "packages/common/protobuf-compiler/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/common/protobuf-compiler/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/common/protobuf-compiler/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "prebuild": {
      "dependsOn": [
        "^build",
        "^compile"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild"
      },
      "outputs": [
        "packages/common/protobuf-compiler/test/proto/gen"
      ]
    },
    "test": {
      "dependsOn": [
        "prebuild"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "test"
      }
    }
  },
  "$schema": "../../../node_modules/nx/schemas/project-schema.json"
}
