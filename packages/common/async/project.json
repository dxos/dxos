{
  "sourceRoot": "packages/common/async/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "packages/common/async/dist"
      ],
      "options": {
        "script": "build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/common/async/**/*.{ts,js}"
        ]
      }
    }
  }
}
