{
  "sourceRoot": "packages/common/async/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/common/async/dist",
        "tsConfig": "packages/common/async/tsconfig.json",
        "main": "packages/common/async/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/common/async/**/*.{ts,js}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "packages/common/async/coverage"
      ],
      "options": {
        "jestConfig": "packages/common/async/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
