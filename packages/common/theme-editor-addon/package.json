    "storybook": "^8.3.6"
  },
  "devDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/react-ui-theme": "workspace:*",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "storybook": "^8.3.6"
  },
  "peerDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/react-ui-theme": "workspace:*",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  }
