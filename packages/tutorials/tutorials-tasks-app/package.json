    "@dxos/react-client": "workspace:*",
    "@dxos/react-components": "workspace:*",
    "@dxos/react-framework": "workspace:*",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/styles": "^5.0.1",
