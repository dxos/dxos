    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "^1.0.5",
    "@types/jest": "^25.2.3",
    "del-cli": "^3.0.0",
    "eslint-plugin-jest": "^24.0.2",
    "jest": "^26.2.2",
    "stream-buffers": "^3.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.3"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
