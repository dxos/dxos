{
  "sourceRoot": "packages/gem/gem-spore/src",
  "projectType": "library",
  "tags": ["scope:gem"],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/gem/gem-spore/dist",
        "tsConfig": "packages/gem/gem-spore/tsconfig.json",
        "main": "packages/gem/gem-spore/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/gem/gem-spore/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@dxos/mocha:mocha",
      "outputs": [
        "{options.outputPath}",
        "{options.resultsPath}"
      ],
      "options": {
        "testPatterns": [
          "packages/gem/gem-spore/src/**/*.test.{ts,js}"
        ],
        "watchPatterns": [
          "packages/gem/gem-spore/src/**/*"
        ],
        "outputPath": "tmp/mocha/gem/gem-spore",
        "resultsPath": "test-results/gem/gem-spore"
      }
    }
  },
  "implicitDependencies": [
    "mocha"
  ]
}
