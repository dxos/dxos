{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "cli-base",
  "tags": [
    "scope:devtools"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "bin": {
      "dependsOn": [
        "build"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "bin"
      }
    },
    "build": {},
    "compile": {
      "options": {
        "entryPoints": [
          "{projectRoot}/src/index.ts"
        ]
      }
    },
    "lint": {}
  },
  "implicitDependencies": [
    "esbuild",
    "node-std"
  ]
}
