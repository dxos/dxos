{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "cli-base",
  "tags": ["scope:devtools"],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "compile": {
      "options": {
        "entryPoints": ["{projectRoot}/src/index.ts"]
      }
    },
    "build": {},
    "bin": {
      "dependsOn": ["build"],
      "executor": "nx:run-script",
      "options": {
        "script": "bin"
      }
    },
    "lint": {}
  },
  "implicitDependencies": [
    "esbuild",
    "node-std"
  ]
}
