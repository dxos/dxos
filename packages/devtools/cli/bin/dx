#!/usr/bin/env -S node --no-warnings

# The following line may have been overridden at the time of `npm publish` or `pnpm deploy`.
process.env.DX_ENVIRONMENT = process.env.DX_ENVIRONMENT ?? 'production'

const oclif = require('@oclif/core')

oclif.run().then(require('@oclif/core/flush')).catch(require('@oclif/core/handle'))
