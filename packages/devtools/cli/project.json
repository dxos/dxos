{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "cli",
  "tags": [
    "scope:devtools"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "bin": {
      "dependsOn": [
        "build"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "bin"
      }
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "options": {
        "outputPath": "{projectRoot}/dist"
      }
    },
    "lint": {},
    "test": {
      "options": {
        "checkLeaks": false,
        "ciEnvironments": [
          "nodejs"
        ]
      }
    }
  },
  "implicitDependencies": [
    "esbuild",
    "test"
  ]
}
