{
  "name": "@dxos/cli",
  "version": "0.8.3",
  "description": "DXOS CLI",
  "keywords": [
    "dxos",
    "cli",
    "oclif"
  ],
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "sideEffects": false,
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dx": "./bin/dx.js"
  },
  "files": [
    "/bin",
    "/config",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "bin": "./bin/dx.js",
    "manifest": "NODE_OPTIONS='--import=extensionless/register' oclif manifest",
    "prepack": "npm run manifest && npm run readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepublishOnly": "scripts/patch_dx_environment.sh",
    "readme": "oclif readme --multi"
  },
  "dependencies": {
    "@dxos/async": "workspace:*",
    "@dxos/cli-base": "workspace:*",
    "@dxos/cli-composer": "workspace:*",
    "@dxos/client": "workspace:*",
    "@dxos/client-protocol": "workspace:*",
    "@dxos/client-services": "workspace:*",
    "@dxos/compute": "workspace:*",
    "@dxos/config": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/echo": "workspace:*",
    "@dxos/functions": "workspace:*",
    "@dxos/functions-runtime": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/network-manager": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/observability": "workspace:*",
    "@dxos/process": "workspace:*",
    "@dxos/protocols": "workspace:*",
    "@dxos/random": "workspace:*",
    "@dxos/rpc": "workspace:*",
    "@dxos/schema": "workspace:*",
    "@dxos/util": "workspace:*",
    "@oclif/core": "catalog:",
    "@oclif/plugin-autocomplete": "catalog:",
    "@oclif/plugin-help": "catalog:",
    "@oclif/plugin-plugins": "catalog:",
    "@oclif/plugin-update": "catalog:",
    "@octokit/core": "catalog:",
    "chalk": "^4.1.0",
    "cli-progress": "catalog:",
    "date-fns": "catalog:",
    "do-wrapper": "catalog:",
    "extensionless": "catalog:",
    "fs-extra": "catalog:",
    "get-folder-size": "catalog:",
    "git-rev-sync": "catalog:",
    "ink": "catalog:",
    "ink-table": "catalog:",
    "inquirer": "catalog:",
    "isomorphic-ws": "catalog:",
    "js-yaml": "catalog:",
    "kubo-rpc-client": "catalog:",
    "lodash.defaultsdeep": "catalog:",
    "node-clipboardy": "catalog:",
    "node-fetch": "catalog:",
    "pkg-up": "catalog:",
    "platform": "catalog:",
    "react": "catalog:",
    "ws": "catalog:",
    "wtfnode": "catalog:"
  },
  "devDependencies": {
    "@oclif/test": "catalog:",
    "@types/cli-progress": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.defaultsdeep": "catalog:",
    "@types/node-fetch": "catalog:",
    "@types/platform": "catalog:",
    "@types/react-dom": "catalog:",
    "oclif": "catalog:",
    "shx": "catalog:",
    "ts-node": "catalog:",
    "undici": "catalog:"
  },
  "peerDependencies": {
    "undici": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "dx",
    "commands": "./dist/src/commands",
    "dirname": "dx",
    "helpClass": "./dist/src/help",
    "hooks": {
      "command_not_found": "./dist/src/hooks/command-not-found"
    },
    "plugins": [
      "@dxos/cli-composer",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "agent": {
        "description": "Manage agents."
      },
      "debug": {
        "description": "Debugging tools and diagnostics."
      },
      "device": {
        "description": "Manage HALO devices."
      },
      "function": {
        "description": "Manage distributed functions and triggers."
      },
      "halo": {
        "description": "Manage HALO identity and devices."
      },
      "space": {
        "description": "Manage ECHO Spaces."
      }
    }
  }
}
