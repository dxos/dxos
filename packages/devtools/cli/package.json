{
  "name": "@dxos/cli",
  "version": "0.1.51",
  "description": "DXOS CLI",
  "keywords": [
    "dxos",
    "cli",
    "oclif"
  ],
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dx": "./bin/run"
  },
  "files": [
    "/bin",
    "/config",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "api-keys": "node ./scripts/inject_api_keys.js",
    "bin": "./bin/run",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "readme": "oclif readme"
  },
  "dependencies": {
    "@dxos/agent": "workspace:*",
    "@dxos/async": "workspace:*",
    "@dxos/bare-template": "workspace:*",
    "@dxos/client": "workspace:*",
    "@dxos/client-protocol": "workspace:*",
    "@dxos/client-services": "workspace:*",
    "@dxos/codec-protobuf": "workspace:*",
    "@dxos/config": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/hello-template": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/observable-object": "workspace:*",
    "@dxos/plate": "workspace:*",
    "@dxos/protocols": "workspace:*",
    "@dxos/rpc": "workspace:*",
    "@dxos/sentry": "workspace:*",
    "@dxos/tasks-template": "workspace:*",
    "@dxos/telemetry": "workspace:*",
    "@dxos/util": "workspace:*",
    "@dxos/websocket-rpc": "workspace:*",
    "@faker-js/faker": "^7.3.0",
    "@oclif/core": "^2.6.5",
    "@oclif/plugin-help": "^5.2.8",
    "@oclif/plugin-plugins": "^2.4.3",
    "@octokit/core": "^4.0.4",
    "chalk": "^4.1.0",
    "cli-progress": "^3.11.2",
    "date-fns": "^2.29.3",
    "debug": "^4.3.3",
    "do-wrapper": "^4.5.1",
    "fs-extra": "^8.1.0",
    "get-folder-size": "^2.0.1",
    "inquirer": "^8.2.0",
    "ipfs-http-client": "~56.0.1",
    "isomorphic-ws": "^4.0.1",
    "js-yaml": "^4.1.0",
    "lodash.defaultsdeep": "^4.6.1",
    "node-fetch": "^2.6.0",
    "oclif": "^3.7.0",
    "pkg-up": "^3.1.0",
    "react": "^18.2.0",
    "tempy": "^1.0.1",
    "uuid": "^8.3.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.11",
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.4",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.5.10",
    "globby": "^11.1.0",
    "nodemon": "^2.0.22",
    "shx": "^0.3.3",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "dx",
    "dirname": "dx",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Configuration information."
      }
    }
  }
}
