{
  "sourceRoot": "packages/devtools/inspector-extension/src",
  "projectType": "application",
  "targets": {
    "bundle": {
      "executor": "nx:run-script",
      "options": {
        "script": "bundle"
      },
      "outputs": [
        "packages/devtools/inspector-extension/dist"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/inspector-extension/**/*.{ts,tsx,js,jsx}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}
