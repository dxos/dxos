{
  "sourceRoot": "packages/devtools/devtools-extension/src",
  "projectType": "application",
  "targets": {
    "bundle-skip": {
      "executor": "@nrwl/vite:build",
      "inputs": [
        {
          "env": "CONFIG_DYNAMIC"
        },
        {
          "env": "DX_ENVIRONMENT"
        },
        {
          "env": "NODE_ENV"
        },
        {
          "env": "SENTRY_DESTINATION"
        },
        {
          "env": "TELEMETRY_API_KEY"
        },
        {
          "env": "IPDATA_API_KEY"
        }
      ],
      "options": {
        "outputPath": "packages/devtools/devtools-extension/out/devtools-extension"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/devtools-extension/**/*.{ts,tsx,js,jsx}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}
