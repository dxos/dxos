{
  "sourceRoot": "packages/devtools/inspector-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/devtools/inspector-app/src/index.ts",
        "outputPath": "packages/devtools/inspector-app/dist/types",
        "tsConfig": "packages/devtools/inspector-app/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "build-storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:build",
      "options": {
        "config": {
          "configFolder": "packages/devtools/inspector-app/.storybook"
        },
        "outputPath": "packages/devtools/inspector-app/out/dxos/storybook/inspector",
        "uiFramework": "@storybook/react"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "bundle": {
      "executor": "@nxext/vite:build",
      "inputs": [
        {
          "env": "CONFIG_DYNAMIC"
        },
        {
          "env": "DX_ENVIRONMENT"
        },
        {
          "env": "NODE_ENV"
        },
        {
          "env": "SENTRY_DESTINATION"
        },
        {
          "env": "TELEMETRY_API_KEY"
        },
        {
          "env": "IPDATA_API_KEY"
        },
        "^production",
        "production"
      ],
      "options": {
        "configFile": "packages/devtools/inspector-app/vite.config.ts",
        "outputPath": "packages/devtools/inspector-app/out/inspector"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "compile": {
      "executor": "@dxos/esbuild:build",
      "options": {
        "entryPoints": [
          "packages/devtools/inspector-app/src/index.ts"
        ],
        "outputPath": "packages/devtools/inspector-app/dist/lib"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/inspector-app/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "configFile": "packages/devtools/inspector-app/vite.config.ts",
        "outputPath": "packages/devtools/inspector-app/out/inspector"
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "config": {
          "configFolder": "packages/devtools/inspector-app/.storybook"
        },
        "uiFramework": "@storybook/react"
      }
    }
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
