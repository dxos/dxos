  "devDependencies": {
    "@babel/core": "~7.13.15",
    "@dxos/esbuild-plugins": "workspace:*",
    "@dxos/esbuild-server": "~1.7.4",
    "@dxos/toolchain-node-library": "workspace:*",
    "@emotion/css": "^11.7.1",
    "@types/prosemirror-schema-basic": "^1.0.2",
    "@types/prosemirror-state": "^1.2.8",
    "@types/prosemirror-view": "^1.23.1",
    "@types/react": "^17.0.24",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@emotion/css": ">=11.0.0",
