{
  "sourceRoot": "packages/devtools/devtools/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/devtools/devtools/src/index.ts",
        "outputPath": "packages/devtools/devtools/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/devtools/devtools/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "build-storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:build",
      "options": {
        "config": {
          "configFolder": "packages/devtools/devtools/.storybook"
        },
        "outputPath": "packages/devtools/devtools/out/dxos/storybook/devtools",
        "uiFramework": "@storybook/react"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "bundle": {
      "executor": "@nxext/vite:build",
      "inputs": [
        {
          "env": "CONFIG_DYNAMIC"
        },
        {
          "env": "DX_ENVIRONMENT"
        },
        {
          "env": "NODE_ENV"
        },
        {
          "env": "SENTRY_DESTINATION"
        },
        {
          "env": "TELEMETRY_API_KEY"
        },
        {
          "env": "IPDATA_API_KEY"
        },
        "^production",
        "production"
      ],
      "options": {
        "configFile": "packages/devtools/devtools/vite.config.ts",
        "outputPath": "packages/devtools/devtools/out/devtools"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/devtools/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "configFile": "packages/devtools/devtools/vite.config.ts",
        "outputPath": "packages/devtools/devtools/out/devtools"
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "config": {
          "configFolder": "packages/devtools/devtools/.storybook"
        },
        "uiFramework": "@storybook/react"
      }
    }
  }
}
