{
  "sourceRoot": "packages/devtools/devtools-testbench/src",
  "projectType": "application",
  "targets": {
    "bundle-disabled-on-ci": {
      "executor": "@nrwl/vite:build",
      "options": {
        "outputPath": "packages/devtools/devtools-testbench/out/devtools-testbench"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/devtools-testbench/**/*.{ts,tsx,js,jsx}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "options": {
        "buildTarget": "devtools-testbench:bundle-disabled-on-ci"
      }
    }
  }
}
