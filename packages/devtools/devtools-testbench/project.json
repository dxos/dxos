{
  "sourceRoot": "packages/devtools/devtools-testbench/src",
  "projectType": "application",
  "targets": {
    "bundle": {
      "executor": "@nxext/vite:build",
      "inputs": [
        {
          "env": "CONFIG_DYNAMIC"
        },
        {
          "env": "DX_ENVIRONMENT"
        },
        {
          "env": "NODE_ENV"
        },
        {
          "env": "SENTRY_DESTINATION"
        },
        {
          "env": "TELEMETRY_API_KEY"
        },
        {
          "env": "IPDATA_API_KEY"
        },
        "^production",
        "production"
      ],
      "options": {
        "configFile": "packages/devtools/devtools-testbench/vite.config.ts",
        "outputPath": "packages/devtools/devtools-testbench/out/devtools"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/devtools-testbench/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "configFile": "packages/devtools/devtools-testbench/vite.config.ts",
        "outputPath": "packages/devtools/devtools-testbench/out/devtools"
      }
    }
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
