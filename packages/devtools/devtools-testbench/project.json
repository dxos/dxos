{
  "sourceRoot": "packages/devtools/devtools-testbench/src",
  "projectType": "application",
  "targets": {
    "bundle": {
      "executor": "@nxext/vite:build",
      "options": {
        "configFile": "packages/devtools/devtools-testbench/vite.config.ts",
        "outputPath": "packages/devtools/devtools-testbench/out/testbench"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/devtools/devtools-testbench/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "configFile": "packages/devtools/devtools-testbench/vite.config.ts",
        "outputPath": "packages/devtools/devtools-testbench/out/testbench"
      }
    }
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
