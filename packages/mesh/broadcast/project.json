{
  "sourceRoot": "packages/mesh/broadcast/src",
  "projectType": "library",
  "targets": {
    "proto": {
      "executor": "@dxos/protobuf-compiler:generate",
      "outputs": [
        "{options.basePath}/gen"
      ],
      "options": {
        "basePath": "packages/mesh/broadcast/src/proto"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/mesh/broadcast/dist",
        "tsConfig": "packages/mesh/broadcast/tsconfig.json",
        "main": "packages/mesh/broadcast/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/mesh/broadcast/**/*.{ts,js}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "packages/mesh/broadcast/coverage"
      ],
      "options": {
        "jestConfig": "packages/mesh/broadcast/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "implicitDependencies": [
    "protobuf-compiler"
  ]
}
