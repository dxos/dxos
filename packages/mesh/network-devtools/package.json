{
  "name": "@dxos/network-devtools",
  "version": "2.10.13",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "storybook": "start-storybook -s ./stories/static -p 9001 -c .storybook",
    "lint": "eslint '{src, stories}/**/*.{ts,js,tsx}'",
    "build": "tsc",
    "build:test": "pnpm run build && pnpm run lint"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/react",
    "rules": {
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "import/export": "off"
    }
  },
  "dependencies": {
    "@dxos/crypto": "workspace:*",
    "@dxos/gem-core": "~1.0.0-beta.25",
    "@dxos/gem-spore": "~1.0.0-beta.25",
    "@dxos/network-manager": "workspace:*",
    "@dxos/protocol-plugin-presence": "workspace:*",
    "@dxos/react-ux": "workspace:*",
    "@geut/discovery-swarm-webrtc": "^4.0.2",
    "@material-ui/core": "^4.11.4",
    "@types/react": "^16.14.0",
    "@wirelineio/discovery-swarm-memory": "^0.2.17",
    "react-resize-aware": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.13.15",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@dxos/eslint-plugin": "~1.0.10",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addons": "^6.3.4",
    "@storybook/api": "^6.3.4",
    "@storybook/components": "^6.3.4",
    "@storybook/core-events": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "^6.3.4",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "~6.2.5",
    "react": "^16.14.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.14.0",
    "ts-loader": "~8.1.0",
    "typescript": "^4.3.5",
    "webpack": "4.44.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
