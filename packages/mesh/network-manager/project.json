{
  "sourceRoot": "packages/mesh/network-manager/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/mesh/network-manager/dist",
        "tsConfig": "packages/mesh/network-manager/tsconfig.json",
        "main": "packages/mesh/network-manager/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/mesh/network-manager/**/*.{ts,js}"
        ]
      }
    },
    "test": {
      "executor": "@dxos/mocha:mocha",
      "options": {
        "testPatterns": [
          "packages/mesh/network-manager/src/**/*.test.{ts,js}"
        ]
      }
    },
    "e2e": {
      "executor": "@dxos/mocha:browser",
      "options": {
        "testPatterns": [
          "packages/mesh/network-manager/src/network-manager.browser-test.ts"
        ],
        "browsers": [
          "chromium",
          "firefox"
        ],
        "setup": "packages/mesh/network-manager/tests-setup.js"
      }
    }
  },
  "implicitDependencies": [
    "mocha"
  ]
}
