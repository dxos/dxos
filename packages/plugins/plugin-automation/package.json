{
  "name": "@dxos/plugin-automation",
  "version": "0.8.3",
  "description": "Prompt chain plugin",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "sideEffects": true,
  "type": "module",
  "sourceExports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./cli": {
      "default": "./src/cli/index.ts"
    },
    "./hooks": {
      "default": "./src/hooks/index.ts"
    },
    "./types": {
      "default": "./src/types/index.ts"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs",
      "node": "./dist/lib/node-esm/index.mjs"
    },
    "./cli": {
      "types": "./dist/types/src/cli/index.d.ts",
      "node": "./dist/lib/node-esm/cli/index.mjs"
    },
    "./hooks": {
      "types": "./dist/types/src/hooks/index.d.ts",
      "browser": "./dist/lib/browser/hooks/index.mjs",
      "node": "./dist/lib/node-esm/hooks/index.mjs"
    },
    "./types": {
      "types": "./dist/types/src/types/index.d.ts",
      "browser": "./dist/lib/browser/types/index.mjs",
      "node": "./dist/lib/node-esm/types/index.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "hooks": [
        "dist/types/src/hooks/index.d.ts"
      ],
      "types": [
        "dist/types/src/types/index.d.ts"
      ]
    }
  },
  "files": [
    "assets",
    "dist",
    "src"
  ],
  "dependencies": {
    "@dxos/ai": "workspace:*",
    "@dxos/app-framework": "workspace:*",
    "@dxos/assistant": "workspace:*",
    "@dxos/async": "workspace:*",
    "@dxos/cli-util": "workspace:*",
    "@dxos/client": "workspace:*",
    "@dxos/conductor": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/echo": "workspace:*",
    "@dxos/effect": "workspace:*",
    "@dxos/functions": "workspace:*",
    "@dxos/functions-runtime": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/live-object": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/operation": "workspace:*",
    "@dxos/plugin-client": "workspace:*",
    "@dxos/plugin-deck": "workspace:*",
    "@dxos/plugin-graph": "workspace:*",
    "@dxos/plugin-space": "workspace:*",
    "@dxos/random": "workspace:*",
    "@dxos/react-client": "workspace:*",
    "@dxos/react-ui-components": "workspace:*",
    "@dxos/react-ui-editor": "workspace:*",
    "@dxos/react-ui-form": "workspace:*",
    "@dxos/react-ui-list": "workspace:*",
    "@dxos/react-ui-stack": "workspace:*",
    "@dxos/schema": "workspace:*",
    "@dxos/types": "workspace:*",
    "@dxos/util": "workspace:*",
    "@effect/ai": "catalog:",
    "@effect/cli": "catalog:",
    "@effect/platform-browser": "catalog:",
    "@effect/printer-ansi": "catalog:",
    "@preact-signals/safe-react": "catalog:",
    "@preact/signals-core": "catalog:"
  },
  "devDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/storybook-utils": "workspace:*",
    "@dxos/ui-theme": "workspace:*",
    "@effect-atom/atom-react": "catalog:",
    "@effect/platform": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "effect": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "vite": "catalog:"
  },
  "peerDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/ui-theme": "workspace:*",
    "@effect-atom/atom-react": "catalog:",
    "@effect/platform": "catalog:",
    "effect": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
