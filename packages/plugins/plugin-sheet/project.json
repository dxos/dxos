{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "plugin-sheet",
  "tags": ["scope:plugins"],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {},
    "compile": {
      "options": {
        "entryPoints": [
          "{projectRoot}/src/index.ts",
          "{projectRoot}/src/meta.tsx",
          "{projectRoot}/src/testing.tsx",
          "{projectRoot}/src/types.ts"
        ],
        "platforms": ["browser", "node"]
      }
    },
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "./scripts/vendor.mjs"
      }
    },
    "lint": {},
    "test": {}
  },
  "implicitDependencies": ["esbuild"]
}
