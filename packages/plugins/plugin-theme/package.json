{
  "name": "@dxos/plugin-theme",
  "version": "0.8.3",
  "description": "DXOS Surface plugin for providing the application theme.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs"
    },
    "./meta": {
      "source": "./src/meta.ts",
      "types": "./dist/types/src/meta.d.ts",
      "browser": "./dist/lib/browser/meta.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "meta": [
        "dist/types/src/meta.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@dxos/app-framework": "workspace:*",
    "@dxos/echo": "workspace:*",
    "@dxos/echo-schema": "workspace:*",
    "@dxos/live-object": "workspace:*",
    "@dxos/util": "workspace:*",
    "@preact-signals/safe-react": "^0.9.0",
    "effect": "3.17.7"
  },
  "devDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/react-ui-theme": "workspace:*",
    "@dxos/storybook-utils": "workspace:*",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "vite": "7.1.1"
  },
  "peerDependencies": {
    "@dxos/react-ui": "workspace:*",
    "@dxos/react-ui-theme": "workspace:*",
    "effect": "^3.13.3",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
