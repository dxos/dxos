{
  "sourceRoot": "packages/deprecated/react-toolkit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/deprecated/react-toolkit/src/index.ts",
        "outputPath": "packages/deprecated/react-toolkit/dist/types",
        "tsConfig": "packages/deprecated/react-toolkit/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "compile": {
      "executor": "@dxos/esbuild:build",
      "options": {
        "entryPoints": [
          "packages/deprecated/react-toolkit/src/index.ts"
        ],
        "outputPath": "packages/deprecated/react-toolkit/dist/lib"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/deprecated/react-toolkit/**/*.{ts,tsx,js,jsx}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
