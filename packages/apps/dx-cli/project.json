{
  "sourceRoot": "packages/apps/dx-cli/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/apps/dx-cli/**/*.{ts,js}"
        ]
      }
    },
    "test": {
      "executor": "@dxos/mocha:mocha",
      "options": {
        "testPatterns": [
          "packages/apps/dx-cli/src/**/*.test.{ts,tsx,js,jsx}"
        ]
      },
      "dependsOn": [
        "build"
      ]
    }
  }
}
