{
  "sourceRoot": "packages/apps/templates/hello-template/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "outputs": [
        "packages/apps/templates/hello-template/dist"
      ]
    },
    "gen": {
      "executor": "nx:run-script",
      "options": {
        "script": "gen"
      },
      "outputs": [
        "packages/apps/templates/hello-template/test/variants"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/apps/templates/hello-template/{src,test}/**/*.{ts,js,tsx,jsx}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}
