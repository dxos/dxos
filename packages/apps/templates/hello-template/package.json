{
  "name": "@dxos/hello-template",
  "version": "0.1.31",
  "description": "An application template for local-first progressive web apps",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "main": "dist/src/config.t.js",
  "scripts": {
    "build": "tsc && plate --inheritance=false --executeFileTemplates=false --output=dist/src src",
    "build-resources": "plate --printMessage=false --inheritance=false --executeFileTemplates=false --output=dist/src src",
    "build-tsc": "tsc",
    "clean": "rm -rf test/out",
    "gen": "test/generate",
    "plate": "plate"
  },
  "dependencies": {
    "@dxos/bare-template": "workspace:*",
    "@dxos/plate": "workspace:*",
    "postcss": "^8.4.21",
    "tailwindcss": "~3.2.7"
  },
  "devDependencies": {
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
