{
  "name": "@dxos/tasks-template",
  "version": "0.1.51",
  "description": "An application template demonstrating a working Task List using the ECHO database",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "main": "dist/src/config.t.js",
  "scripts": {
    "build": "tsc && plate --inheritance=false --executeFileTemplates=false --output=dist/src src",
    "build-resources": "plate --printMessage=false --inheritance=false --executeFileTemplates=false --output=dist/src src",
    "build-tsc": "tsc",
    "clean": "rm -rf test/out",
    "gen": "test/generate"
  },
  "dependencies": {
    "@dxos/bare-template": "workspace:*",
    "@dxos/plate": "workspace:*"
  },
  "devDependencies": {
    "@dxos/aurora": "workspace:*",
    "@dxos/aurora-theme": "workspace:*",
    "@dxos/client": "workspace:*",
    "@dxos/react-appkit": "workspace:*",
    "@dxos/react-client": "workspace:*",
    "@phosphor-icons/react": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
