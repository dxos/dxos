{
  "sourceRoot": "packages/apps/test-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "options": {
        "main": "packages/apps/test-app/src/index.ts",
        "outputPath": "packages/apps/test-app/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/apps/test-app/tsconfig.json"
      }
    },
    "bundle": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "packages/apps/test-app/out/test-app"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/apps/test-app/**/*.{ts,js}?(x)"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "test-app:bundle"
      }
    },
    "serve-with-vault": {
      "dependsOn": [
        "^build"
      ],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx serve halo-app"
          },
          {
            "command": "nx serve test-app"
          }
        ],
        "parallel": true
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nx/storybook:storybook",
      "options": {
        "config": {
          "configFolder": "packages/ui/react-ui/.storybook"
        },
        "uiFramework": "@storybook/react"
      }
    }
  }
}
