{
  "sourceRoot": "packages/apps/types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/apps/types/src/index.ts",
        "outputPath": "packages/apps/types/dist/types",
        "tsConfig": "packages/apps/types/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "compile": {
      "executor": "@dxos/esbuild:build",
      "options": {
        "entryPoints": [
          "packages/apps/types/src/index.ts"
        ],
        "injectGlobals": true,
        "outputPath": "packages/apps/types/dist/lib"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/apps/types/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "prebuild": {
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild"
      },
      "outputs": [
        "packages/apps/types/src/proto/gen"
      ]
    }
  }
}
