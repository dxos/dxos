{
  "sourceRoot": "packages/apps/braneframe/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/apps/braneframe/src/index.ts",
        "outputPath": "packages/apps/braneframe/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/apps/braneframe/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "bundle": {
      "executor": "@nrwl/vite:build",
      "inputs": [
        {
          "env": "CONFIG_DYNAMIC"
        },
        {
          "env": "DX_ENVIRONMENT"
        },
        {
          "env": "DX_VAULT"
        },
        {
          "env": "NODE_ENV"
        },
        {
          "env": "SENTRY_DESTINATION"
        },
        {
          "env": "TELEMETRY_API_KEY"
        },
        {
          "env": "IPDATA_API_KEY"
        },
        "^production",
        "production"
      ],
      "options": {
        "outputPath": "packages/apps/braneframe/out/braneframe"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/apps/braneframe/**/*.{ts,js}?(x)"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "options": {
        "buildTarget": "braneframe:bundle"
      }
    },
    "serve-with-halo": {
      "executor": "@dxos/serve-with-halo:run"
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "config": {
          "configFolder": "packages/apps/braneframe/.storybook"
        },
        "uiFramework": "@storybook/react"
      }
    }
  },
  "implicitDependencies": [
    "serve-with-halo"
  ]
}
