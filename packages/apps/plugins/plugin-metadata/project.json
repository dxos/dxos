{
  "sourceRoot": "packages/apps/plugins/plugin-metadata/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "main": "packages/apps/plugins/plugin-metadata/src/index.ts",
        "outputPath": "packages/apps/plugins/plugin-metadata/dist/types",
        "tsConfig": "packages/apps/plugins/plugin-metadata/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "compile": {
      "executor": "@dxos/esbuild:build",
      "options": {
        "entryPoints": [
          "packages/apps/plugins/plugin-metadata/src/index.ts",
          "packages/apps/plugins/plugin-metadata/src/meta.ts"
        ],
        "outputPath": "packages/apps/plugins/plugin-metadata/dist/lib"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/apps/plugins/plugin-metadata/src/**/*.{js,jsx,ts,tsx}"
        ],
        "quiet": true
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "packages/apps/plugins/plugin-metadata/.storybook"
      }
    }
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
