    "@dxos/debug": "workspace:*"
  },
  "devDependencies": {
    "@dxos/app-framework": "workspace:*",
    "@dxos/react-client": "workspace:*",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "@dxos/app-framework": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
