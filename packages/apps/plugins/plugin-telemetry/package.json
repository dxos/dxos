{
  "name": "@braneframe/plugin-telemetry",
  "version": "0.3.7",
  "description": "DXOS Surface plugin for hooking up app telemetry",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "exports": {
