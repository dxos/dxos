{
  "name": "@braneframe/plugin-client",
  "version": "0.5.4",
  "description": "DXOS Surface plugin for DXOS Client",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "DXOS.org",
  "exports": {
    ".": {
      "browser": "./dist/lib/browser/index.mjs",
      "import": "./dist/lib/browser/index.mjs",
      "require": "./dist/lib/node/index.cjs",
      "node": "./dist/lib/node/index.cjs"
    },
    "./meta": {
      "browser": "./dist/lib/browser/meta.mjs",
      "import": "./dist/lib/browser/meta.mjs",
      "require": "./dist/lib/node/meta.cjs",
      "node": "./dist/lib/node/meta.cjs"
    },
    "./space-properties": {
      "browser": "./dist/lib/browser/space-properties.mjs",
      "import": "./dist/lib/browser/space-properties.mjs",
      "require": "./dist/lib/node/space-properties.cjs",
      "node": "./dist/lib/node/space-properties.cjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "meta": [
        "dist/types/src/meta.d.ts"
      ],
      "space-properties": [
        "dist/types/src/space-properties.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@braneframe/types": "workspace:*",
    "@dxos/config": "workspace:*",
    "@dxos/echo-signals": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/local-storage": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/react-client": "workspace:*",
    "@dxos/react-ui": "workspace:*",
    "@phosphor-icons/react": "^2.1.5"
  },
  "devDependencies": {
    "@dxos/app-framework": "workspace:*",
    "@dxos/storybook-utils": "workspace:*",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.2.9"
  },
  "peerDependencies": {
    "@dxos/app-framework": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
