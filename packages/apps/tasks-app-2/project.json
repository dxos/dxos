{
  "sourceRoot": "packages/apps/tasks-app-2/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/apps/tasks-app-2/**/*.{ts,js}?(x)"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve-with-halo": {
      "dependsOn": [
        "^compile"
      ],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx serve halo-app"
          },
          {
            "command": "nx serve tasks-app-2"
          }
        ],
        "parallel": true
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "configDir": "packages/ui/aurora/.storybook",
        "uiFramework": "@storybook/react"
      }
    }
  }
}
