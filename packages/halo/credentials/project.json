{
  "sourceRoot": "packages/halo/credentials/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "packages/halo/credentials/dist",
        "packages/halo/credentials/src/proto/gen"
      ],
      "options": {
        "script": "build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/halo/credentials/**/*.{ts,js}"
        ]
      }
    }
  }
}
