{
  "sourceRoot": "packages/halo/credentials/src",
  "projectType": "library",
  "targets": {
    "proto": {
      "executor": "@dxos/protobuf-compiler:generate",
      "outputs": [
        "{options.basePath}{options.outputPath}"
      ],
      "options": {
        "basePath": "packages/halo/credentials/src/proto/"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/halo/credentials/dist",
        "tsConfig": "packages/halo/credentials/tsconfig.json",
        "main": "packages/halo/credentials/src/index.ts",
        "updateBuildableProjectDepsInPackageJson": false,
        "assets": ["packages/halo/credentials/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/halo/credentials/**/*.{ts,js}"
        ]
      }
    }
  },
  "implicitDependencies": [
    "protobuf-compiler"
  ]
}
