{
  "name": "DXOS",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "DXOS Wallet Extension",
  "icons": {
      "512": "dxos.png"
  },
  "browser_action": {
      "default_icon": "dxos.png",
      "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  }
}
