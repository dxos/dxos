{
  "name": "@dxos/echo-signals",
  "version": "0.8.3",
  "description": "Plugable integration between ECHO and Signals libraries.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "FSL-1.1-Apache-2.0",
  "author": "info@dxos.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    "./core": {
      "source": "./src/core.ts",
      "types": "./dist/types/src/core.d.ts",
      "browser": "./dist/lib/browser/core.mjs",
      "node": "./dist/lib/node-esm/core.mjs"
    },
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs",
      "node": "./dist/lib/node-esm/index.mjs"
    },
    "./react": {
      "source": "./src/react.ts",
      "types": "./dist/types/src/react.d.ts",
      "browser": "./dist/lib/browser/react.mjs",
      "node": "./dist/lib/node-esm/react.mjs"
    },
    "./runtime": {
      "source": "./src/runtime/index.ts",
      "types": "./dist/types/src/runtime/index.d.ts",
      "browser": "./dist/lib/browser/runtime/index.mjs",
      "node": "./dist/lib/node-esm/runtime/index.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "core": [
        "dist/types/src/core.d.ts"
      ],
      "react": [
        "dist/types/src/react.d.ts"
      ],
      "runtime": [
        "dist/types/src/runtime/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@preact/signals": "^2.3.1",
    "@preact/signals-core": "^1.12.1",
    "@preact/signals-react": "^3.3.0"
  },
  "devDependencies": {
    "preact": "^10.26.9",
    "react": "~19.2.0"
  },
  "optionalDependencies": {
    "preact": "^10.26.9",
    "react": "^19.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
