{
  "sourceRoot": "packages/core/echo/echo-db/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/core/echo/echo-db/dist",
        "tsConfig": "packages/core/echo/echo-db/tsconfig.json",
        "main": "packages/core/echo/echo-db/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "eslintConfig": "packages/core/echo/echo-db/eslint-config.json",
        "lintFilePatterns": [
          "packages/core/echo/echo-db/src/packlets/**/*.{ts,js}"
        ]
      }
    },
    "test": {
      "executor": "@dxos/mocha:mocha",
      "outputs": [
        "{options.outputPath}",
        "{options.resultsPath}"
      ],
      "options": {
        "testPatterns": [
          "packages/core/echo/echo-db/src/**/*.test.{ts,js}"
        ],
        "watchPatterns": [
          "packages/core/echo/echo-db/src/**/*"
        ],
        "outputPath": "tmp/mocha/core/echo/echo-db",
        "resultsPath": "test-results/core/echo/echo-db",
        "setup": "packages/core/mesh/signal/setup.js",
        "forceExit": true
      }
    }
  },
  "implicitDependencies": [
    "mocha"
  ]
}
