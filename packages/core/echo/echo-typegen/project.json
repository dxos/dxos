{
  "sourceRoot": "packages/core/echo/echo-typegen/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/core/echo/echo-typegen/src/main.ts",
        "outputPath": "packages/core/echo/echo-typegen/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/core/echo/echo-typegen/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "prebuild": {
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild",
        "transformers": [
          "@dxos/log-hook/transformer"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/core/echo/echo-typegen/src/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "dependsOn": [
        "^prebuild"
      ],
      "executor": "@dxos/test:run",
      "options": {
        "coveragePath": "coverage/packages/core/echo/echo-typegen",
        "forceExit": true,
        "outputPath": "tmp/mocha/packages/core/echo/echo-typegen",
        "resultsPath": "test-results/packages/core/echo/echo-typegen",
        "testPatterns": [
          "packages/core/echo/echo-typegen/src/**/*.test.{ts,js}"
        ],
        "watchPatterns": [
          "packages/core/echo/echo-typegen/src/**/*"
        ]
      },
      "outputs": [
        "{options.coveragePath}",
        "{options.outputPath}",
        "{options.resultsPath}"
      ]
    }
  },
  "implicitDependencies": [
    "esbuild",
    "test"
  ]
}
