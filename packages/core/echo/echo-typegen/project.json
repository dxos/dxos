{
  "sourceRoot": "packages/core/echo/echo-typegen/src",
  "projectType": "library",
  "targets": {
    "compile": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/core/protocols/src/main.ts",
        "outputPath": "packages/core/protocols/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/core/protocols/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/core/echo/echo-typegen/src/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "prebuild": {
      "dependsOn": [
        "^build",
        "^compile"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild",
        "transformers": [
          "@dxos/log-hook/transformer"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "test": {
      "dependsOn": [
        "prebuild"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "test"
      }
    }
  }
}
