{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "automerge",
  "tags": [
    "scope:echo"
  ],
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {},
    "compile": {
      "executor": "nx:run-script",
      "inputs": [
        "{projectRoot}/scripts/**/*",
        "{projectRoot}/src/**/*"
      ],
      "options": {
        "script": "compile"
      },
      "outputs": [
        "{projectRoot}/dist/lib"
      ]
    },
    "lint": {}
  },
  "implicitDependencies": [
    "esbuild"
  ]
}
