{
  "name": "@dxos/protocols",
  "version": "0.1.23",
  "description": "Protobuf definitions for DXOS protocols.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "exports": {
    ".": {
      "browser": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./proto/*": {
      "browser": "./dist/esm/src/proto/gen/*.js",
      "require": "./dist/cjs/src/proto/gen/*.js"
    }
  },
  "types": "dist/esm/src/index.d.ts",
  "typesVersions": {
    "*": {
      "proto/*": [
        "dist/esm/src/proto/gen/*"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "tsc-cjs": "tsc --module commonjs --target es2018 --outdir ./dist/cjs"
  },
  "dependencies": {
    "@dxos/codec-protobuf": "workspace:*",
    "@dxos/hypercore": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/timeframe": "workspace:*"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "debug": "^4.3.3",
    "glob": "~7.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
