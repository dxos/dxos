{
  "name": "@dxos/functions-runtime",
  "version": "0.8.3",
  "description": "Functions runtime.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
  "author": "info@dxos.org",
  "sideEffects": false,
  "type": "module",
  "sourceExports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./native": {
      "default": "./src/native/index.ts"
    },
    "./bundler": {
      "default": "./src/bundler/index.ts"
    },
    "./edge": {
      "default": "./src/edge/index.ts"
    },
    "./testing": {
      "default": "./src/testing/index.ts"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs",
      "node": "./dist/lib/node-esm/index.mjs"
    },
    "./native": {
      "types": "./dist/types/src/native/index.d.ts",
      "browser": "./dist/lib/browser/native/index.mjs",
      "node": "./dist/lib/node-esm/native/index.mjs"
    },
    "./bundler": {
      "types": "./dist/types/src/bundler/index.d.ts",
      "browser": "./dist/lib/browser/bundler/index.mjs",
      "node": "./dist/lib/node-esm/bundler/index.mjs"
    },
    "./edge": {
      "types": "./dist/types/src/edge/index.d.ts",
      "browser": "./dist/lib/browser/edge/index.mjs",
      "node": "./dist/lib/node-esm/edge/index.mjs"
    },
    "./testing": {
      "types": "./dist/types/src/testing/index.d.ts",
      "browser": "./dist/lib/browser/testing/index.mjs",
      "node": "./dist/lib/node-esm/testing/index.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "bundler": [
        "dist/types/src/bundler/index.d.ts"
      ],
      "edge": [
        "dist/types/src/edge/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/lib",
    "dist/types",
    "schema",
    "src"
  ],
  "scripts": {
    "publish-runtime": "node ./scripts/build-modules.mjs && node ./scripts/upload-modules.mjs --env dev"
  },
  "dependencies": {
    "@dxos/ai": "workspace:*",
    "@dxos/async": "workspace:*",
    "@dxos/client": "workspace:*",
    "@dxos/client-protocol": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/echo": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/echo-pipeline": "workspace:*",
    "@dxos/echo-protocol": "workspace:*",
    "@dxos/edge-client": "workspace:*",
    "@dxos/effect": "workspace:*",
    "@dxos/errors": "workspace:*",
    "@dxos/functions": "workspace:*",
    "@dxos/functions-runtime-cloudflare": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/kv-store": "workspace:*",
    "@dxos/live-object": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/protocols": "workspace:*",
    "@dxos/schema": "workspace:*",
    "@dxos/util": "workspace:*",
    "@effect/platform": "catalog:",
    "effect": "catalog:",
    "esbuild": "catalog:",
    "esbuild-wasm": "catalog:",
    "i18next": "catalog:",
    "iso-did": "catalog:",
    "jsonata": "catalog:"
  },
  "devDependencies": {
    "@automerge/automerge": "catalog:",
    "@aws-sdk/client-s3": "3.955.0",
    "@dxos/ai": "workspace:*",
    "@dxos/types": "workspace:*",
    "chess.js": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
