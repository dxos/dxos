{
  "sourceRoot": "packages/core/agent/src",
  "projectType": "library",
  "targets": {
    "bin": {
      "dependsOn": [
        "build"
      ],
      "executor": "nx:run-script",
      "options": {
        "script": "bin"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "packages/core/agent/src/index.ts",
        "outputPath": "packages/core/agent/dist",
        "transformers": [
          "@dxos/log-hook/transformer"
        ],
        "tsConfig": "packages/core/agent/tsconfig.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/core/agent/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@dxos/test:run",
      "options": {
        "checkLeaks": false,
        "ciEnvironments": [
          "nodejs"
        ],
        "coveragePath": "coverage/packages/core/agent",
        "outputPath": "tmp/mocha/packages/core/agent",
        "resultsPath": "test-results/packages/core/agent",
        "testPatterns": [
          "packages/core/agent/src/**/*.test.{ts,js}"
        ],
        "watchPatterns": [
          "packages/core/agent/src/**/*"
        ]
      },
      "outputs": [
        "{options.coveragePath}",
        "{options.outputPath}",
        "{options.resultsPath}"
      ]
    }
  },
  "implicitDependencies": [
    "esbuild",
    "test"
  ]
}
