{
  "name": "@dxos/assistant",
  "version": "0.8.3",
  "description": "Natural language interface to DXOS",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dxos/dxos"
  },
  "license": "MIT",
  "author": "info@dxos.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/types/src/index.d.ts",
      "default": "./dist/lib/neutral/index.mjs"
    },
    "./extraction": {
      "source": "./src/extraction/index.ts",
      "types": "./dist/types/src/extraction/index.d.ts",
      "default": "./dist/lib/neutral/extraction/index.mjs"
    },
    "./testing": {
      "source": "./src/testing/index.ts",
      "types": "./dist/types/src/testing/index.d.ts",
      "default": "./dist/lib/neutral/testing/index.mjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "types": [
        "dist/types/src/types/index.d.ts"
      ],
      "extraction": [
        "dist/types/src/extraction/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "run:chat": "vite-node ./bin/chat.ts"
  },
  "dependencies": {
    "@automerge/automerge": "catalog:",
    "@dxos/ai": "workspace:*",
    "@dxos/async": "workspace:*",
    "@dxos/blueprints": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/echo": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/echo-protocol": "workspace:*",
    "@dxos/effect": "workspace:*",
    "@dxos/errors": "workspace:*",
    "@dxos/functions": "workspace:*",
    "@dxos/functions-runtime": "workspace:*",
    "@dxos/graph": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/schema": "workspace:*",
    "@dxos/types": "workspace:*",
    "@dxos/util": "workspace:*",
    "@effect/ai": "catalog:",
    "@effect/experimental": "catalog:",
    "@effect/platform": "catalog:",
    "@xenova/transformers": "catalog:",
    "lodash.defaultsdeep": "catalog:"
  },
  "devDependencies": {
    "@effect-atom/atom": "catalog:",
    "@effect-atom/atom-react": "catalog:",
    "@types/lodash.defaultsdeep": "catalog:",
    "effect": "catalog:"
  },
  "peerDependencies": {
    "@effect-atom/atom": "catalog:",
    "@effect-atom/atom-react": "catalog:",
    "effect": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "beast": {}
}
