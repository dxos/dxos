{
  "name": "@dxos/demos",
  "version": "2.10.10",
  "private": true,
  "description": "ECHO database demo app.",
  "homepage": "https://github.com/dxos/protocols/tree/master/packages/echo",
  "bugs": {
    "url": "'https://github.com/dxos/protocols/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/protocols.git"
  },
  "license": "AGPL-3.0",
  "author": "DXOS.org",
  "scripts": {
    "build": "tsc --noEmit",
    "build-storybook": "build-storybook --quiet",
    "build-storybook-docs": "build-storybook --quiet --docs",
    "build:test": "pnpm run build && pnpm run lint && pnpm run test 2>&1",
    "chromatic": "npx chromatic --exit-zero-on-changes --auto-accept-changes",
    "chromatic-docs": "npx chromatic --exit-zero-on-changes --auto-accept-changes --build-script-name build-storybook-docs",
    "depcheck": "npx depcheck --specials=bin,babel,eslint,lint-staged",
    "grid:demo": "mocha -r dotenv/config -r ts-node/register/transpile-only './grid/**/*.test.ts'",
    "lint": "toolchain lint",
    "prepublishOnly": "npm run test && npm run build",
    "storybook": "start-storybook -s ./stories/static -p 9001 -c .storybook --ci",
    "storybook-docs": "start-storybook --docs --no-manager-cache",
    "test": "exit 0",
    "test:playwright": "mocha -r ts-node/register/transpile-only './playwright/**/*.test.ts'",
    "watch": "babel-watch src --out-dir ./dist --source-maps"
  },
  "browserslist": [
    "> 5%"
  ],
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/echo-protocol": "workspace:*",
    "@dxos/echo-testing": "workspace:*",
    "@dxos/gem-core": "~1.0.0-beta.25",
    "@dxos/gem-spore": "~1.0.0-beta.25",
    "@dxos/object-model": "workspace:*",
    "@dxos/react-client": "workspace:*",
    "@dxos/react-framework": "workspace:*",
    "@dxos/util": "workspace:*",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/styles": "^5.0.1",
    "@types/debug": "^4.1.1",
    "clsx": "^1.1.0",
    "d3": "^5.9.2",
    "debug": "^4.1.1",
    "immutability-helper": "^3.0.2",
    "mobile-detect": "~1.4.5",
    "playwright": "^1.7.0",
    "react-resize-aware": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "~7.13.15",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@dxos/async": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/toolchain-node-library": "workspace:*",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/components": "^6.3.4",
    "@storybook/core-events": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "^6.3.4",
    "@types/faker": "~5.5.1",
    "@types/mocha": "~8.2.2",
    "@types/node": "^14.0.9",
    "@types/pump": "^1.1.0",
    "@types/react": "^17.0.24",
    "babel-loader": "8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "buffer": "^6.0.3",
    "chromatic": "^5.8.0",
    "dotenv": "^8.2.0",
    "expect": "~27.0.2",
    "mocha": "~8.4.0",
    "pump": "^3.0.0",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "robotjs": "~0.6.0",
    "ts-loader": "~8.1.0",
