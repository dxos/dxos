{
  "name": "@dxos/tutorials-tasks-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true craco build",
    "build:test": "npm run build && npm run test",
    "eject": "craco eject",
    "pack:app": "pnpm pack",
    "start": "SKIP_PREFLIGHT_CHECK=true craco start",
    "test": "SKIP_PREFLIGHT_CHECK=true craco test --passWithNoTests --watchAll=false",
    "test:playwright": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register/transpile-only './playwright/**/*.test.ts' --timeout 30000"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
