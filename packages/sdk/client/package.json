{
  "name": "@dxos/client",
  "version": "0.1.45",
  "description": "Core DXOS Client API.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "repository": "github:dxos/dxos",
  "license": "MIT",
  "author": "DXOS.org",
  "exports": {
    ".": {
      "browser": "./dist/lib/browser/index.mjs",
      "import": "./dist/lib/browser/index.mjs",
      "require": "./dist/lib/node/index.cjs",
      "node": "./dist/lib/node/index.cjs"
    },
    "./testing": {
      "browser": "./dist/lib/browser/packlets/testing/index.mjs",
      "import": "./dist/lib/browser/packlets/testing/index.mjs",
      "require": "./dist/lib/node/packlets/testing/index.cjs",
      "node": "./dist/lib/node/packlets/testing/index.cjs"
    }
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "testing": [
        "dist/types/src/packlets/testing/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "dxtype src/packlets/proto/schema.proto src/packlets/proto/gen/schema.ts"
  },
  "dependencies": {
    "@dxos/async": "workspace:*",
    "@dxos/client-protocol": "workspace:*",
    "@dxos/client-services": "workspace:*",
    "@dxos/config": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/credentials": "workspace:*",
    "@dxos/crypto": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/document-model": "workspace:*",
    "@dxos/echo-db": "workspace:*",
    "@dxos/echo-schema": "workspace:*",
    "@dxos/echo-typegen": "workspace:*",
    "@dxos/errors": "workspace:*",
    "@dxos/keys": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/messaging": "workspace:*",
    "@dxos/model-factory": "workspace:*",
    "@dxos/network-manager": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/protocols": "workspace:*",
    "@dxos/rpc": "workspace:*",
    "@dxos/rpc-tunnel": "workspace:*",
    "@dxos/text-model": "workspace:*",
    "@dxos/timeouts": "workspace:*",
    "@dxos/util": "workspace:*",
    "@dxos/websocket-rpc": "workspace:*",
    "base-x": "~3.0.9",
    "debug": "^4.3.3",
    "lodash.isequal": "^4.5.0",
    "lodash.isequalwith": "^4.4.0"
  },
  "devDependencies": {
    "@dxos/codec-protobuf": "workspace:*",
    "@dxos/random-access-storage": "workspace:*",
    "@types/lodash.isequal": "^4.5.0",
    "@types/lodash.isequalwith": "^4.4.0",
    "@types/node": "^18.11.9",
    "fast-check": "~3.3.0",
    "source-map-support": "^0.5.12"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "beast": {
    "classDiagrams": [
      {
        "root": "src/packlets/client/client.ts",
        "dependencies": [
          "@dxos/client-services"
        ]
      }
    ]
  },
  "typedoc": {
    "entryPoint": "src/index.ts"
  }
}
