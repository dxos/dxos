{
  "sourceRoot": "packages/sdk/client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "packages/sdk/client/dist",
        "packages/sdk/client/src/packlets/proto/gen"
      ],
      "options": {
        "script": "build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/sdk/client/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
