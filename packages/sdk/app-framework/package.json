{
  "name": "@dxos/app-framework",
  "version": "0.8.3",
  "description": "A framework for building applications from composible plugins.",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "MIT",
