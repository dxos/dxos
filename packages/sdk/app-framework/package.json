  "peerDependencies": {
    "@effect-atom/atom-react": "catalog:",
    "@effect/platform": "catalog:",
    "effect": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
