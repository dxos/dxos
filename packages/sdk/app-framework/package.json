    "effect": "3.18.3",
    "react": "~19.2.0",
    "typedoc": "0.28.1"
  },
  "peerDependencies": {
    "@effect-rx/rx-react": "^0.34.1",
    "@effect/platform": "^0.80.12",
    "effect": "^3.13.3",
    "react": "^19.0.0"
