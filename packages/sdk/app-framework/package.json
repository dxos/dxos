    "@dxos/local-storage": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/react-hooks": "workspace:*",
    "@dxos/schema": "workspace:*",
    "@dxos/util": "workspace:*",
    "@effect/ai": "0.29.1",
    "@effect/experimental": "0.56.0",
