{
  "sourceRoot": "packages/sdk/client-singleton/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/sdk/client-singleton/dist",
        "tsConfig": "packages/sdk/client-singleton/tsconfig.json",
        "main": "packages/sdk/client-singleton/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/sdk/client-singleton/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "config": {
          "configFolder": "packages/sdk/client-singleton/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "packages/sdk/client-singleton/out/storybook",
        "config": {
          "configFolder": "packages/sdk/client-singleton/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
