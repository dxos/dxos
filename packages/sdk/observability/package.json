{
  "name": "@dxos/observability",
  "version": "0.8.3",
  "description": "Provides a common interface for app and platform observability",
  "homepage": "https://dxos.org",
  "bugs": "https://github.com/dxos/dxos/issues",
  "license": "Apache-2.0",
  "author": "DXOS.org",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/lib/browser/index.mjs",
      "node": "./dist/lib/node-esm/index.mjs"
    },
    "./segment": {
      "source": "./src/segment/index.ts",
      "types": "./dist/types/src/segment/index.d.ts",
      "browser": "./dist/lib/browser/segment/index.mjs",
      "node": "./dist/lib/node-esm/segment/index.mjs"
    },
    "./sentry": {
      "source": "./src/sentry/index.ts",
      "types": "./dist/types/src/sentry/index.d.ts",
      "browser": "./dist/lib/browser/sentry/index.mjs",
      "node": "./dist/lib/node-esm/sentry/index.mjs"
    }
  },
  "browser": {
    "./src/segment/node.ts": "./src/segment/browser.ts",
    "./src/sentry/node.ts": "./src/sentry/browser.ts",
    "./testing/testkit/index.ts": "./testing/testkit/browser.ts",
    "./src/otel/traces.ts": "./src/otel/traces-browser.ts"
  },
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "segment": [
        "dist/types/src/segment/index.d.ts"
      ],
      "sentry": [
        "dist/types/src/sentry/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@dxos/async": "workspace:*",
    "@dxos/client": "workspace:*",
    "@dxos/client-protocol": "workspace:*",
    "@dxos/client-services": "workspace:*",
    "@dxos/config": "workspace:*",
    "@dxos/context": "workspace:*",
    "@dxos/debug": "workspace:*",
    "@dxos/invariant": "workspace:*",
    "@dxos/log": "workspace:*",
    "@dxos/network-manager": "workspace:*",
    "@dxos/node-std": "workspace:*",
    "@dxos/protocols": "workspace:*",
    "@dxos/tracing": "workspace:*",
    "@dxos/util": "workspace:*",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/api-logs": "catalog:",
    "@opentelemetry/auto-instrumentations-web": "catalog:",
    "@opentelemetry/exporter-logs-otlp-http": "catalog:",
    "@opentelemetry/exporter-metrics-otlp-http": "catalog:",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:",
    "@opentelemetry/instrumentation": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-logs": "catalog:",
    "@opentelemetry/sdk-metrics": "catalog:",
    "@opentelemetry/sdk-trace-base": "catalog:",
    "@opentelemetry/sdk-trace-web": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "@segment/analytics-node": "catalog:",
    "@segment/snippet": "catalog:",
    "@sentry/browser": "catalog:",
    "@sentry/node": "catalog:",
    "debug": "catalog:",
    "js-yaml": "catalog:",
    "localforage": "catalog:",
    "uuid": "catalog:"
  },
  "devDependencies": {
    "@sentry/types": "catalog:",
    "@types/debug": "catalog:",
    "@types/js-yaml": "catalog:",
    "sentry-testkit": "catalog:",
    "zone.js": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
