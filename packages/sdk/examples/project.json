{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "examples",
  "projectType": "application",
  "targets": {
    "build": {},
    "lint": {
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "{projectRoot}/**/*.{ts,tsx,js,jsx}"
        ],
        "quiet": true
      }
    },
    "prebuild": {
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild"
      },
      "outputs": [
        "{projectRoot}/src/proto/gen"
      ]
    }
  }
}
