{
  "name": "@dxos/config",
  "version": "2.10.2",
  "description": "Config utilities",
  "keywords": [],
  "license": "AGPLv3",
  "author": "DxOS.org",
  "main": "dist/es/index.js",
  "files": [
    "dist",
    "ConfigPlugin.js"
  ],
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --source-maps inline",
    "build:watch": "babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --source-maps inline --watch",
    "clean": "del-cli dist",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "toolchain lint",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests --no-cache"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "boolean": "^3.0.1",
    "debug": "^4.1.1",
    "js-yaml": "^3.13.1",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "~7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.12.7",
    "babel-loader": "8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-inline-import": "^3.0.0",
    "del-cli": "^3.0.0",
    "@dxos/toolchain-node-library": "workspace:*",
    "jest": "^26.2.2",
    "webpack": "4.44.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
