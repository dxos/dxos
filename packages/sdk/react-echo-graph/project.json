{
  "sourceRoot": "packages/sdk/react-echo-graph/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/sdk/react-echo-graph/dist",
        "tsConfig": "packages/sdk/react-echo-graph/tsconfig.json",
        "main": "packages/sdk/react-echo-graph/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "packages/sdk/react-echo-graph/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
