{
  "sourceRoot": "packages/demos/hello-world/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/demos/hello-world/dist",
        "tsConfig": "packages/demos/hello-world/tsconfig.json",
        "main": "packages/demos/hello-world/src/index.ts"
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "packages/demos/hello-world/out/storybook",
        "config": {
          "configFolder": "packages/demos/hello-world/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "bundle": {
      "executor": "@nxext/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/demos/hello-world/out/hello-world",
        "configFile": "packages/demos/hello-world/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/demos/hello-world/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "packages/demos/hello-world/out/hello-world",
        "configFile": "packages/demos/hello-world/vite.config.ts"
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "config": {
          "configFolder": "packages/demos/hello-world/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
