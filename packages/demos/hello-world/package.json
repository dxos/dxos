{
  "name": "@dxos/hello-world",
  "version": "2.33.4",
  "private": true,
  "description": "Hello World.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/protocols.git"
  },
  "license": "MIT",
  "author": "DXOS.org",
  "scripts": {
    "book": "esbuild-server book --mdx",
    "build": "tsc --noEmit && npm run book -- --build",
    "build:test": "pnpm run build && pnpm run lint && pnpm run test 2>&1",
    "depcheck": "npx depcheck --specials=bin,babel,eslint,lint-staged",
    "lint": "toolchain lint",
    "prepublishOnly": "npm run test && npm run build",
    "start": "toolchain start",
    "test": "exit 0",
    "test:playwright": "mocha -r ts-node/register/transpile-only './playwright/**/*.test.ts'",
    "watch": "babel-watch src --out-dir ./dist --source-maps"
  },
  "browserslist": [
    "> 5%"
  ],
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/config": "workspace:*",
    "@dxos/object-model": "workspace:*",
    "@dxos/react-client": "workspace:*",
    "@dxos/react-components": "workspace:*",
    "@dxos/react-toolkit": "workspace:*",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "~7.13.15",
    "@dxos/esbuild-plugins": "~2.28.7",
    "@dxos/esbuild-server": "~2.28.8",
    "@dxos/eslint-plugin": "~1.0.31",
    "@dxos/protocols-toolchain": "workspace:*",
    "@dxos/react-client-testing": "workspace:*",
    "@types/mocha": "~8.2.2",
    "@types/node": "^16.11.27",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint": "^7.12.1",
    "expect": "~27.0.2",
    "mocha": "~8.4.0",
    "playwright": "^1.7.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
