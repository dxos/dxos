{
  "name": "@dxos/dx-cli",
  "version": "2.33.8",
  "description": "CLI Test using oclif.",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dx": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "toolchain build",
    "check": "toolchain check",
    "lint": "toolchain lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "run:config": "npm run build && ./bin/run config --config ./config/config.yml --json",
    "test": "toolchain test",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/config": "workspace:*",
    "@oclif/core": "^1.14.1",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@octokit/core": "^4.0.4",
    "chalk": "^4.1.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^4.1.0",
    "inquirer": "~9.0.2",
    "ink": "^3.2.0",
    "react": "^18.1.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "workspace:*",
    "@dxos/protocols-toolchain": "workspace:*",
    "@oclif/test": "^2.1.1",
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.11.27",
    "chai": "^4.3.4",
    "eslint": "^8.21.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "globby": "^11.1.0",
    "mocha": "^8.4.0",
    "oclif": "^3.1.2",
    "shx": "^0.3.3",
    "ts-node": "^10.8.2",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "bin": "dx",
    "dirname": "dx",
    "commands": "./dist/src/commands",
    "hooks": {
      "init": [
        "./dist/src/hooks/init/config"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "macos": {
      "identifier": "org.dxos.dx.cli"
    },
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Configuration information."
      },
      "identity": {
        "description": "Manage your decentralized identity."
      },
      "shell": {
        "description": "Interactive shell."
      },
      "space": {
        "description": "Manage your Brane spaces."
      }
    }
  },
  "toolchain": {
    "forceCloseTests": true,
    "testingFramework": "mocha"
  }
}
