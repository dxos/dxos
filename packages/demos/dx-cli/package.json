{
  "name": "@dxos/dx-cli",
  "version": "2.33.8",
  "description": "CLI Test using oclif.",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dx": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "toolchain build",
    "lint": "toolchain lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@dxos/client": "workspace:*",
    "@dxos/config": "workspace:*",
    "@oclif/core": "^1.11.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.0.1",
    "@octokit/core": "^4.0.4",
    "fs-extra": "^8.1.0",
    "js-yaml": "^4.1.0",
    "inquirer": "~9.0.2"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.34",
    "@dxos/protocols-toolchain": "workspace:*",
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.11.27",
    "chai": "^4.3.4",
    "eslint": "^7.12.1",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "globby": "^11.1.0",
    "mocha": "^8.4.0",
    "oclif": "^3.1.1",
    "shx": "^0.3.3",
    "ts-node": "^10.8.2",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "bin": "dx",
    "dirname": "dx",
    "commands": "./dist/commands",
    "hooks": {
      "init": [
        "./dist/hooks/init/config"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Configuration information."
      },
      "identity": {
        "description": "Manage your decentralized identity."
      },
      "kube": {
        "description": "Manage your KUBE device and MESH network."
      },
      "shell": {
        "description": "Interactive shell."
      },
      "space": {
        "description": "Manage your Brane spaces."
      }
    }
  }
}
