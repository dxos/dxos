{
  "sourceRoot": "packages/demos/lexical-editor/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "packages/demos/lexical-editor/dist"
      ],
      "options": {
        "script": "build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/demos/lexical-editor/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
