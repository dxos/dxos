{
  "sourceRoot": "e2e/protobuf-compiler/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "e2e/protobuf-compiler/dist",
        "tsConfig": "e2e/protobuf-compiler/tsconfig.json",
        "main": "e2e/protobuf-compiler/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "e2e/protobuf-compiler/**/*.{ts,js}"
        ]
      }
    }
    // TODO(wittjosiah): This test is broken.
    // "e2e": {
