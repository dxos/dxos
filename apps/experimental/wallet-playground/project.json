{
  "sourceRoot": "apps/experimental/wallet-playground/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/experimental/wallet-playground/dist",
        "tsConfig": "apps/experimental/wallet-playground/tsconfig.json",
        "main": "apps/experimental/wallet-playground/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/experimental/wallet-playground/**/*.{ts,js}"
        ]
      }
    }
  }
}
