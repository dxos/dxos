{
  "sourceRoot": "apps/experimental/docs/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "dependsOn": [
        "^build",
        "^bundle",
        "^prerelease",
        "^proto"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/experimental/docs/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}
