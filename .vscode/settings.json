{
  "json.schemas": [
    {
      "fileMatch": ["packages/wallet/extension/public/manifest.json"],
      "url": "https://raw.githubusercontent.com/PrsPrsBK/ffext-manifest-schema/master/ffext.json"
    }
  ],
  "cSpell.words": [
    "Automerge",
    "bitfield",
    "burdon",
    "cbor",
    "COOLDOWN",
    "CRDT",
    "dedup",
    "dxos",
    "DXOS",
    "excalidraw",
    "Expando",
    "Faas",
    "Hypergraph",
    "keymap",
    "mykola",
    "Orama",
    "protobuf",
    "testutils",
    "typenames",
    "wittjosiah"
  ],
  "files.associations": {
    "workspace.json": "jsonc",
    "schema.json": "jsonc",
    "eslintConfig.json": "jsonc",
    ".eslintrc.json": "jsonc",
    "command-line.json": "jsonc",
    "version-policies.json": "jsonc",
    "artifactory.json": "jsonc",
    "experiments.json": "jsonc",
    "build-cache.json": "jsonc",
    "command-versions.json": "jsonc"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "search.followSymlinks": false,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "hediet.vscode-drawio.theme": "Kennedy",
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "debug.javascript.terminalOptions": {
    "skipFiles": ["**/**/tracing/src/api.ts"]
  },
  "circleci.persistedProjectSelection": [],
  "vitest.disableWorkspaceWarning": true,
  "vitest.rootConfig": "vitest.all.config.ts",
  "typescript.preferences.autoImportSpecifierExcludeRegexes": [
    "^.*/src/.*$",
    "node:console",
    "console",
    "effect/Effect",
    "effect/Console"
  ],
  "circleci.notifications.mine": true,
  "search.useParentIgnoreFiles": true,
  "search.useGlobalIgnoreFiles": false
}
