  "version": "0.2.0",
  "configurations": [
  {
    "args": [
      "--timeout",
      "999999",
      "--colors",
      "-r",
      "@swc-node/register",
      "${workspaceFolder}/packages/echo/echo-db/**/*.test.ts"
    ],
    "internalConsoleOptions": "openOnSessionStart",
    "name": "Mocha Tests: @dxos/echo-db",
    "program": "${workspaceFolder}/toolchains/toolchain-node-library/node_modules/mocha/bin/_mocha",
    "request": "launch",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "cwd": "${workspaceFolder}/packages/common/protobuf-compiler",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "gen"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
