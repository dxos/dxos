{
  "sourceRoot": "tools/executors/mocha/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "tools/executors/mocha/src/main.ts",
        "outputPath": "tools/executors/mocha/dist",
        "tsConfig": "tools/executors/mocha/tsconfig.json",
        "transformers": [
          "@dxos/log-hook/transformer"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "tools/executors/mocha/**/*.{ts,js}"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "toolbox": {
      "executor": "@dxos/toolbox:exec"
    }
  },
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": [
    "log-hook"
  ]
}
