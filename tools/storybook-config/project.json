{
  "sourceRoot": "tools/storybook-config/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "options": {
        "lintFilePatterns": [
          "tools/storybook-config/src/**/*.{ts,tsx,js,jsx}",
          "tools/storybook-config/.storybook/*.{cjs,html}"
        ]
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "tools/storybook-config/.storybook"
      }
    },
    "storybook-build": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nx/storybook:build",
      "options": {
        "configDir": "tools/storybook-config/.storybook",
        "outputDir": "out"
      },
      "outputs": [
        "{options.outputDir}"
      ]
    }
  }
}
