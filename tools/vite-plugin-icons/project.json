{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "vite-plugin-icons",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {
      "dependsOn": [
        "^build",
        "prebuild"
      ],
      "options": {
        "main": "{projectRoot}/src/index.ts",
        "outputPath": "{projectRoot}/dist",
        "tsConfig": "{projectRoot}/tsconfig.json"
      }
    },
    "compile": {
      "dependsOn": [
        "build"
      ],
      "executor": "nx:noop"
    },
    "lint": {}
  }
}
