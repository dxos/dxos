    "new": "new",
    "nx": "nx",
    "prepare": "[ \"$HUSKY\" != \"0\" ] && husky install || true",
    "serve": "serve",
    "test": "nx run-many --target=test",
    "vitest": "nx run-many --target=vitest",
    "watch": "./node_modules/.bin/nx watch --all -- pnpm -w nx compile \"\\$NX_PROJECT_NAME\"",
