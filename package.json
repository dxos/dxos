    "1p-credentials": "bash -c \"op inject -i '$(git rev-parse --show-toplevel)/.env.1password'\"",
    "audit-classes": "pnpm --filter @dxos/ui-theme audit-classes",
    "beast": "beast docs",
    "build": "moon run :build",
    "changed-packages": "scripts/changed",
    "check-cycles": "node scripts/check-cycles.mjs",
    "create-worktree": "./scripts/create-worktree",
    "gh-action": "./scripts/gh-action.mjs",
    "postinstall": "[ \"$CI\" != \"true\" ] && vite-node tools/toolbox/src/main.ts || true",
